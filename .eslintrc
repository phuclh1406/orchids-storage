{
  "env": {
      "browser": true,
      "es6": true
  },
  "parser": "@babel/eslint-parser",
  "extends": "eslint:recommended",
  "parserOptions": {
      "ecmaFeatures": {
          "experimentalObjectRestSpread": true,
          "jsx": true
      },
      "sourceType": "module",
      "ecmaVersion": 2017
  },
  "plugins": [
      "react"
  ],
//   "rules": {
      
//       "indent": [
//           "error",
//           2
//       ],
//       "linebreak-style": [
//           "error",
//           "unix"
//       ],
//       "quotes": [
//           "error",
//           "single"
//       ],
      
//   }
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "comma-dangle": 0,
    "react/jsx-uses-vars": 1,
    "react/display-name": 1,
    "no-unused-vars": "error",
    "no-console": 1,
    "no-unexpected-multiline": "warn"
},
"settings": {
    "react": {
        "pragma": "React",
        "version": "15.6.1"
    }
}
}